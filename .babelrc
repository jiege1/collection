
{
  "presets": [
    "react",
    "env",
    "stage-1"
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "@babel/runtime"
    }],
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
    ["transform-decorators-legacy"]
  ]
}
